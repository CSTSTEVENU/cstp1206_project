          span.navbar-toggler-icon
        #navbarCollapse.collapse.navbar-collapse
          .navbar-nav
            a.nav-item.nav-link.active(href='/') Home
            a.nav-item.nav-link(href='#') Services
            a.nav-item.nav-link(href='/about') About
            a.nav-item.nav-link(href='/contact') Contact
          .navbar-nav.ms-auto
            a.nav-item.nav-link(href='/login') Login
            a.nav-item.nav-link(href='/register') Register 
    // End header


    block content     

  

    // footer
    footer.container-fluid.navbar-fixed-bottom
      p
        | Scrapbook is powered by
        span Team8
        |  /
        span Contact us
    // End footer

